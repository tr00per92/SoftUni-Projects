@model Photography.Web.ViewModels.HomePageViewModel
@{
    ViewBag.Title = "Home";
}

<div class="jumbotron">
    <h1 class="text-center">Photography System</h1>
</div>

<div class="row">
    <div class="col-md-6">
        <h1 class="text-center">Last photographs</h1>
        @Html.Partial("_PhotosList", Model.LatestPhotos)
    </div>

    <div class="col-md-6">
        <h1 class="text-center">Last albums</h1>
        <br />
        @Html.DisplayFor(m => m.LatestAlbums)
    </div>
</div>
